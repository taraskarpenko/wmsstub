extends layout

block content
    h1 WMS simulator for NewStore integration
    p List of received requests
    table(id="requests_table",)
        tr
            th Customer
            th Request ID
            th Created at
                br
                | (from payload)
            th Received at
                br
                | (local time)
            th products in request
            th Details
        each value, index in requests
            tr
                td= value.customer_n1 + " " + value.customer_n2
                td= value.request_id
                td= value.created_at
                td= value.received_at
                td
                    each val1 in value.products.split(',')
                        p= val1.replace('\"','').replace('\"','')
                td
                    a(href= "/stub/" + value.request_id) >>>>>>>>

